{
  "OTIS Clubs": {
    "prefix": "points",
    "description": "Adds points to the club counter.",
    "body": [
      "\\addtocounter{clubcounter}{${1:Point Value}}"
    ]
  },
  "Proof": {
    "prefix": "proof",
    "description": "Inserts a proof environment",
    "body": [
      "\\begin{proof}",
      "  ${1:Content}",
      "\\end{proof}$2"
    ]
  },
  "Theorem": {
    "prefix": "theorem",
    "description": "Inserts a theorem environment",
    "body": [
      "\\begin{theorem}",
      "  ${1:Content}",
      "\\end{theorem}$2"
    ]
  },
  "Claim": {
    "prefix": "claim",
    "description": "Inserts a claim environment",
    "body": [
      "\\begin{claim*}",
      "  ${1:Content}",
      "\\end{claim*}$2"
    ]
  }
}
